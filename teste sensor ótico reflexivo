// Sensor linha preta
int pinIrC = 12;


void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
  pinMode(pinIrC, INPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
  int valorLidoC = digitalRead(pinIrC);

    Serial.print(" Centro ");
  Serial.println(valorLidoC);

}
